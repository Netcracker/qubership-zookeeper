{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch operator",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "console": "integratedTerminal",
      "program": "${workspaceFolder}\\zookeeper-service-operator\\main.go",
      "env": {
          "HOME":"${userHome}",
          "KUBECONFIG": "<path_to_your_config>",
          "WATCH_NAMESPACE":"<your_zookeeper_namespace>"
      }
  }
  ]
}